#include <iostream>
using namespace std;

int main() {
    int row = 2, col = 3;
    int arr[row][col];

    for(int i = 0; i < row; i++)
        for(int j = 0; j < col; j++)
            cin >> arr[i][j];

    for(int i = 0; i < row; i++) {
        int sum = 0;
        for(int j = 0; j < col; j++)
            sum += arr[i][j];
        cout << "Row " << i << " sum = " << sum << endl;
    }

    return 0;
}
